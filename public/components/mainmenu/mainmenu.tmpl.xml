<fest:template xmlns:fest="http://fest.mail.ru" context_name="data">
  <div class="container">
    <div class="row">
      <div class="col-md-12 col-xs-12" style="height:150px"></div>
    </div>
    <div class="col-md-12 col-xs-12"  style="height:50px">
      <img class="waiting-sign" alt="50x50" src="./assets/stary.svg" hidden="true"/>
    </div>
  </div>
  <div class="col-md-12 col-xs-12"  style="height:20px"></div>

  <div class="col-md-2 col-md-offset-5 col-xs-4 col-xs-offset-4 " align="center">
    <fest:for iterate="data.buttons" index="i" value="button">
      <button id="{button.id}" tabindex="{button.tabindex}" type="button" class="btn btn-success btn-margin" style="width:200px"><fest:value>button.text</fest:value></button>
      <div class="col-md-12 col-xs-12" style="height:20px"></div>
    </fest:for>
  </div>
  </fest:template>
